{
  "name": "@true-and-useful/janee",
  "version": "0.1.2",
  "description": "Secrets management for AI agents via MCP",
  "main": "dist/index.js",
  "bin": {
    "janee": "dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rsdouglas/janee.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc && npm run build --workspaces",
    "build:cli": "tsc",
    "build:plugin": "npm run build --workspace=packages/openclaw-plugin",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ai",
    "agents",
    "security",
    "secrets",
    "mcp",
    "model-context-protocol",
    "api-proxy"
  ],
  "author": "True and Useful LLC",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/js-yaml": "^4.0.9",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
